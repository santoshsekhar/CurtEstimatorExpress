<div id="materialList">
  
    <span id="materialCount">
      
    </span> Material Entries
   
      <div id="oneMaterial">
        <p></p>
        <div class=row>
          <div class="col col-xs-4">
            <div class="form-group">
              <label for="product" name="productLabel">Material </label>
              <input type="text" class="form-control" id="" name="product" value="">
            </div>
          </div>
          <div class="col col-xs-4">
            <div class="form-group">
              <label for="unitcost" name="unitcostLabel">$/unit</label>
              <input onchange="onEstimateChange()" type="number" class="form-control" id="" name="unitcost" step="1" value=""
                min=0 max=500000>
            </div>
          </div>
          <div class="col col-xs-4">
            <div class="form-group">
              <label for="coverageSquareFeetPerUnit" name="coverageLabel">Sqft/unit</label>
              <input onchange="onEstimateChange()" type="number" class="form-control" id="" step="1"
                value="" name="coverageSquareFeetPerUnit" min=1 max=50000>
            </div>
          </div>
        </div>
        <span id=""></span> units =
        <span id=""></span>
        <p></p>
      </div>
     
</div>
